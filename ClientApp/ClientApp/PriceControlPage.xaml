      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ClientApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PriceControl"
      Loaded="Page_Loaded">

    <Grid Background="White">

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button Grid.Row="0" x:Name="btnBack" Content="Back" HorizontalAlignment="Left" Margin="47,28,0,0" VerticalAlignment="Top" Click="btnBack_Click"/>
        <TextBlock Grid.Row="1" x:Name="txtHeader" Margin="47,10,571,336" FontSize="18" FontWeight="Bold">Price Control</TextBlock>
        <DataGrid Grid.Row="1" x:Name="dgItems" Margin="47,66,309,45" />

    </Grid>
</Page>
